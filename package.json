{
    "name": "go-with-me",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:style": "prettier --write ./src",
        "prepare": "husky install",
        "codegen": "graphql-codegen --config codegen.ts",
        "migration:create": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate create --",
        "migration:up": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate up --",
        "migration:down": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate down --",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@apollo/client": "^3.12.9",
        "@apollo/server": "^4.11.3",
        "@as-integrations/next": "^3.2.0",
        "@auth/mongodb-adapter": "^3.7.4",
        "@aws-sdk/client-s3": "^3.741.0",
        "@aws-sdk/s3-request-presigner": "^3.741.0",
        "@googlemaps/js-api-loader": "^1.16.8",
        "@googlemaps/routing": "^1.4.0",
        "@graphql-tools/schema": "^10.0.16",
        "@playwright/experimental-ct-react": "^1.51.1",
        "@vis.gl/react-google-maps": "^1.5.1",
        "child_process": "^1.0.2",
        "dayjs": "^1.11.13",
        "graphql": "^16.10.0",
        "graphql-tag": "^2.12.6",
        "graphql-type-json": "^0.3.2",
        "luxon": "^3.5.0",
        "mongodb": "^6.13.0",
        "mongoose": "^8.10.0",
        "nanoid": "^5.0.9",
        "next": "^15.3.0",
        "next-auth": "^4.24.11",
        "nodemailer": "^6.10.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.54.2",
        "simpledotcss": "^2.3.3",
        "ts-migrate-mongoose": "^3.8.9"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.4",
        "@faker-js/faker": "^9.4.0",
        "@playwright/test": "^1.50.1",
        "@storybook/addon-essentials": "^8.5.3",
        "@storybook/addon-interactions": "^8.5.3",
        "@storybook/addon-links": "^8.5.3",
        "@storybook/addon-onboarding": "^8.5.3",
        "@storybook/blocks": "^8.5.3",
        "@storybook/nextjs": "^8.5.3",
        "@storybook/react": "^8.5.3",
        "@storybook/test": "^8.5.3",
        "@svgr/webpack": "^8.1.0",
        "@types/google.maps": "^3.58.1",
        "@types/luxon": "^3.4.2",
        "@types/node": "^22.13.1",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "eslint": "^9.19.0",
        "eslint-config-next": "15.1.6",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-storybook": "^0.11.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.3",
        "prettier": "^3.4.2",
        "storybook": "^8.5.3",
        "typescript": "^5.7.3"
    }
}
