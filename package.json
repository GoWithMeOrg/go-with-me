{
    "name": "go-with-me",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:style": "prettier --write ./src",
        "prepare": "husky install",
        "codegen": "graphql-codegen --config codegen.ts",
        "migration:create": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate create --",
        "migration:up": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate up --",
        "migration:down": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate down --"
    },
    "engines": {
        "node": "20"
    },
    "dependencies": {
        "@apollo/client": "^3.8.9",
        "@apollo/server": "^4.10.0",
        "@as-integrations/next": "^3.0.0",
        "@auth/mongodb-adapter": "^2.0.12",
        "@googlemaps/js-api-loader": "^1.16.6",
        "dayjs": "^1.11.10",
        "graphql": "^16.8.1",
        "graphql-tag": "^2.12.6",
        "luxon": "^3.4.4",
        "mongodb": "^6.3.0",
        "mongoose": "^8.0.4",
        "next": "^14.0.4",
        "next-auth": "^4.24.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "simpledotcss": "^2.2.1",
        "ts-migrate-mongoose": "^3.5.6"
    },
    "devDependencies": {
        "@types/google.maps": "^3.55.4",
        "@types/luxon": "^3.4.0",
        "@types/node": "^20.11.0",
        "@types/react": "^18.2.47",
        "@types/react-dom": "^18.2.18",
        "eslint": "^8.56.0",
        "eslint-config-next": "14.0.4",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.1",
        "typescript": "^5.3.3"
    }
}
