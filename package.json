{
    "name": "go-with-me",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:style": "prettier --write ./src",
        "prepare": "husky install",
        "codegen": "graphql-codegen --config codegen.ts",
        "migration:create": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate create --",
        "migration:up": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate up --",
        "migration:down": "env $(cat .env.local | grep -v \"#\" | xargs) npx migrate down --",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "mcp:start": "npx mongodb-mcp-server",
        "mcp:install": "npm install --save-dev mongodb-mcp-server"
    },
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "@apollo/client": "^4.0.7",
        "@apollo/server": "^5.0.0",
        "@as-integrations/next": "^4.0.0",
        "@auth/mongodb-adapter": "^3.11.0",
        "@aws-sdk/client-s3": "^3.913.0",
        "@aws-sdk/s3-request-presigner": "^3.913.0",
        "@googlemaps/js-api-loader": "^2.0.1",
        "@googlemaps/routing": "^2.1.1",
        "@graphql-tools/schema": "^10.0.25",
        "@vis.gl/react-google-maps": "^1.5.5",
        "child_process": "^1.0.2",
        "dayjs": "^1.11.18",
        "graphql": "^16.11.0",
        "graphql-tag": "^2.12.6",
        "graphql-type-json": "^0.3.2",
        "luxon": "^3.7.2",
        "mongodb": "^6.20.0",
        "mongoose": "^8.19.2",
        "nanoid": "^5.1.6",
        "next": "^15.5.7",
        "next-auth": "^4.24.11",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hook-form": "^7.65.0",
        "simpledotcss": "^2.3.7",
        "ts-migrate-mongoose": "^4.1.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^10.1.0",
        "@modelcontextprotocol/sdk": "^1.20.1",
        "@playwright/test": "^1.56.1",
        "@storybook/addon-docs": "^9.1.13",
        "@storybook/addon-links": "^9.1.13",
        "@storybook/addon-onboarding": "^9.1.13",
        "@storybook/nextjs": "^9.1.13",
        "@svgr/webpack": "^8.1.0",
        "@types/google.maps": "^3.58.1",
        "@types/luxon": "^3.7.1",
        "@types/node": "^24.9.1",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "eslint": "^9.38.0",
        "eslint-config-next": "15.5.6",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-storybook": "^9.1.13",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.5",
        "mongodb-mcp-server": "^1.1.0",
        "prettier": "^3.6.2",
        "storybook": "^9.1.13",
        "typescript": "^5.9.3"
    }
}
