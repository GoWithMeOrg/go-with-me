{
  "endOfLine": "auto",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "es5",
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(modules|lib|mocks|utils|config|assets|temp|styles|ui-kit|src)\\/.*(?<!\\.css)(?<!\\/styles)$",
    "^(\\.|\\.{2}|\\.\\/.*|\\.{2}\\/.*)(?<!\\.css)(?<!\\/styles)$",
    "",
    "(.*\\.css|\\.\\/styles)$"
  ]
}
